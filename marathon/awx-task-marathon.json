{
  "id": "/devops/dev/awx/awx-task",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [],
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/etc/pki/ca-trust/source/anchors",
        "hostPath": "/mnt/nfs/awx_data/ca-trust/source/anchors",
        "mode": "RO"
      },
      {
        "containerPath": "/root/.ssh",
        "hostPath": "/mnt/nfs/awx_data/.ssh",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "ansible/awx_task:1.0.6",
      "forcePullImage": false,
      "privileged": true,
      "parameters": [
        {
          "key": "hostname",
          "value": "awx"
        },
        {
          "key": "user",
          "value": "root"
        }
      ]
    }
  },
  "cpus": 4,
  "disk": 0,
  "env": {
    "DATABASE_USER": {
      "secret": "postgres_user"
    },
    "DATABASE_PASSWORD": {
      "secret": "postgres_password"
    },
    "AWX_ADMIN_PASSWORD": {
      "secret": "admin_password"
    },
    "SECRET_KEY": {
      "secret": "secret_key"
    },
    "AWX_ADMIN_USER": {
      "secret": "admin_user"
    },
    "RABBITMQ_USER": "guest",
    "RABBITMQ_PORT": "5672",
    "DATABASE_HOST": "awx-postgres.marathon.l4lb.thisdcos.directory",
    "RABBITMQ_VHOST": "awx",
    "MEMCACHED_HOST": "awx-memcached.marathon.l4lb.thisdcos.directory",
    "DATABASE_NAME": "awx",
    "DATABASE_PORT": "5432",
    "MEMCACHED_PORT": "11211",
    "RABBITMQ_HOST": "awx-rabbitmq.marathon.l4lb.thisdcos.directory",
    "RABBITMQ_PASSWORD": "guest"
  },
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 4096,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "secrets": {
    "postgres_user": {
      "source": "devops/dev/awx/awx_postgres_user"
    },
    "postgres_password": {
      "source": "devops/dev/awx/awx_postgres_password"
    },
    "admin_password": {
      "source": "devops/dev/awx/awx_admin_password"
    },
    "secret_key": {
      "source": "devops/dev/awx/awx_secret_key"
    },
    "admin_user": {
      "source": "devops/dev/awx/awx_admin_user"
    }
  },
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "healthChecks": [],
  "fetch": [],
  "constraints": []
}