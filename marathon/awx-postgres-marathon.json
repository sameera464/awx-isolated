{
  "labels": {
    "DCOS_PACKAGE_RELEASE": "1",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_REGISTRY_VERSION": "3.0",
    "DCOS_SERVICE_NAME": "postgresql",
    "DCOS_PACKAGE_VERSION": "9.6-0.2",
    "DCOS_PACKAGE_NAME": "postgresql",
    "MARATHON_SINGLE_INSTANCE_APP": "true",
    "DCOS_PACKAGE_IS_FRAMEWORK": "false"
  },
  "id": "/devops/dev/awx/postgres",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [{
      "containerPort": 5432,
      "hostPort": 0,
      "labels": {
        "VIP_0": "/awx-postgres:5432"
      },
      "protocol": "tcp",
      "servicePort": 0,
      "name": "awx-postgres"
    }],
    "type": "DOCKER",
    "volumes": [{
      "containerPath": "/var/lib/postgresql/data/",
      "hostPath": "/mnt/nfs/awx_data/postgres_data",
      "mode": "RW"
    }],
    "docker": {
      "image": "postgres:9.6",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 0.3,
  "disk": 0,
  "env": {
    "POSTGRES_USER": {
      "secret": "postgres_user"
    },
    "POSTGRES_PGDATA": "/var/lib/postgresql/data/pgdata",
    "POSTGRES_DB": "awx",
    "POSTGRES_PASSWORD": {
      "secret": "postgres_password"
    }
  },
  "healthChecks": [{
    "gracePeriodSeconds": 300,
    "intervalSeconds": 60,
    "maxConsecutiveFailures": 3,
    "portIndex": 0,
    "timeoutSeconds": 20,
    "delaySeconds": 15,
    "protocol": "TCP",
    "ipProtocol": "IPv4"
  }],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 1024,
  "gpus": 0,
  "networks": [{
    "mode": "container/bridge"
  }],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "fetch": [],
  "constraints": [],
  "secrets": {
    "postgres_user": {
      "source": "devops/dev/awx/awx_postgres_user"
    },
    "postgres_password": {
      "source": "devops/dev/awx/awx_postgres_password"
    }
  }
}